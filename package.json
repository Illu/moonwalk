{
  "name": "moonwalk",
  "version": "2.3.0",
  "private": true,
  "repository": "https://github.com/Illu/moonwalk",
  "author": "Maxime Nory",
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "build-ios": "react-native bundle --entry-file='index.js' --bundle-output='./ios/main.jsbundle' --assets-dest ./ios --dev=false --platform='ios'",
    "test": "TZ=America/New_York LC_ALL=\"en_US.UTF-8\" jest",
    "lint": "eslint .",
    "prettier-format": "prettier --write '**/*.{js,jsx,ts,tsx,vue,json,css,scss,less,md,mdx,graphql}'",
    "prettier-check": "prettier --debug-check '**/*.{js,jsx,ts,tsx,vue,json,css,scss,less,md,mdx,graphql}'"
  },
  "dependencies": {
    "@react-native-community/async-storage": "^1.8.1",
    "@react-native-community/masked-view": "^0.1.6",
    "@react-native-community/push-notification-ios": "^1.1.0",
    "@react-navigation/bottom-tabs": "^5.1.0",
    "@react-navigation/native": "^5.0.0",
    "@react-navigation/stack": "^5.0.0",
    "mobx": "^5.15.4",
    "mobx-react": "^6.1.7",
    "react": "16.9.0",
    "react-native": "0.61.5",
    "react-native-appearance": "^0.3.2",
    "react-native-change-icon": "^2.0.1",
    "react-native-firebase": "^5.6.0",
    "react-native-gesture-handler": "^1.5.6",
    "react-native-inappbrowser-reborn": "^3.4.0",
    "react-native-open-maps": "^0.3.5",
    "react-native-push-notification": "^3.1.9",
    "react-native-reanimated": "^1.7.0",
    "react-native-safe-area-context": "^0.7.2",
    "react-native-screens": "^2.0.0-beta.2",
    "react-native-store-review": "^0.1.5",
    "react-native-svg": "^11.0.1",
    "react-native-svg-transformer": "^0.14.3",
    "react-navigation": "^4.3.2",
    "react-navigation-stack": "^2.3.4",
    "styled-components": "^5.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/runtime": "^7.8.4",
    "@react-native-community/eslint-config": "^0.0.7",
    "@testing-library/jest-native": "^3.4.3",
    "@testing-library/react-hooks": "^5.0.3",
    "@testing-library/react-native": "^7.1.0",
    "@types/jest": "^25.1.5",
    "@types/react-native": "^0.63.50",
    "@types/styled-components": "^5.1.7",
    "@types/styled-components-react-native": "^5.1.1",
    "@typescript-eslint/parser": "^3.3.0",
    "babel-jest": "^25.1.0",
    "eslint": "^7.2.0",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-testing-library": "^3.3.1",
    "husky": "^4.2.3",
    "jest": "^25.1.0",
    "lint-staged": "^10.1.1",
    "metro-react-native-babel-preset": "^0.58.0",
    "prettier": "^2.0.2",
    "react-test-renderer": "16.9.0",
    "typescript": "^3.9.5"
  },
  "jest": {
    "preset": "react-native",
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && yarn test"
    }
  },
  "lint-staged": {
    "*.{js,json,css,md}": [
      "prettier --write"
    ]
  }
}
